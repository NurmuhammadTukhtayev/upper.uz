<!DOCTYPE html>
<html>
<%- include("../basic1/head.ejs") -%>

    <body>
        <div id="wrapper">
            <%-  include("../basic1/nav.ejs") -%>
                <!-- /. NAV SIDE  -->
                <div id="page-wrapper">
                    <div id="page-inner">
                        <div class="row">
                            <div class="col-md-12">
                                <h1 class="page-head-line">Tilar </h1>
                                <h1 class="page-subhead-line"> Dasturlash tilari . </h1>
                            </div>
                        </div>

                        <div class="row">
                            <div class="col-lg-12 col-md-12 col-sm-12">
                                <!--   Kitchen Sink -->
                                <div class="panel panel-default">
                                    <div class="panel-heading">
                                        Tilar
                                    </div>
                                    <div class="panel-body">
                                        <div class="table-responsive">
                                            <table class="table table-striped table-bordered table-hover">
                                                <thead>
                                                    <tr>
                                                        <th>#</th>
                                                        <th>name</th>
                                                        <th>icon</th>
                                                        <th>type</th>
                                                        <th>D</th>
                                                    </tr>
                                                </thead>
                                                <tbody>
                                                    <% for(var i=0;i< langs.length;i++) { %>
                                                        <tr>
                                                            <th>
                                                                <%= i %>
                                                            </th>
                                                            <td>
                                                                <%= langs[i].name_lang %>
                                                            </td>
                                                            <td>
                                                                <img src="/lang/<%= langs[i].icon_lang %>" alt="" width="40px">
                                                            </td>
                                                            <td>
                                                                <%= langs[i].type %>
                                                            </td>
                                                            <td>
                                                                <a href="/admin/lang/delete/<%= langs[i].id %>"> delete</a>
                                                            </td>

                                                        </tr>
                                                        <% } %> %>

                                                </tbody>
                                            </table>
                                        </div>
                                    </div>
                                </div>
                                <!-- End  Kitchen Sink -->
                            </div>
                        </div>
                    </div>
                </div>
        </div>

        <%- include("../basic1/footer.ejs") -%>
    </body>

</html>